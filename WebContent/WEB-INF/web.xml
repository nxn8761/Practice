<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Practice</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ConnectionPoolingServlet</display-name>
    <servlet-name>ConnectionPoolingServlet</servlet-name>
    <servlet-class>com.naga.demos.ConnectionPoolingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ConnectionPoolingServlet</servlet-name>
    <url-pattern>/ConnectionPoolingServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginCounterServlet</display-name>
    <servlet-name>LoginCounterServlet</servlet-name>
    <servlet-class>com.naga.servlets.LoginCounterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginCounterServlet</servlet-name>
    <url-pattern>/LoginCounterServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ServletLifeCycle</display-name>
    <servlet-name>ServletLifeCycle</servlet-name>
    <servlet-class>com.naga.servlets.ServletLifeCycle</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletLifeCycle</servlet-name>
    <url-pattern>/ServletLifeCycle</url-pattern>
  </servlet-mapping>
</web-app>